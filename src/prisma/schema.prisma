datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id         BigInt   @id @default(autoincrement())
  categoryId BigInt
  blogId     BigInt
  title      String   @db.VarChar(50)
  content    String   @db.Text
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}